{global, orbit heading, h2 []}
{id inline-or-block, h2 [Inline or Block]}
{two [
    {display [Source]}
    {display [Result]}
    {lang stdn, code ['{code [inline]}']}
    {code [inline]}
    {lang stdn, block, code ['{block, code [block]}']}
    {block, code [block]}
    {lang stdn, code [
        '{code ['
        '    block'
        '    block'
        ']}'
    ]}
    {code [
        block
        block
    ]}
]}
